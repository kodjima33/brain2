{
  "name": "@brain2/expo",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "pnpm with-env expo start -c --android",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "prebuild:android": "pnpm with-env expo prebuild --platform android",
    "build:android": "cd android && ./gradlew :app:installRelease",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "with-env": "dotenv -e ../../.env --",
    "set-ip": "REACT_NATIVE_PACKAGER_HOSTNAME=",
    "open-url": "uri-scheme open --android",
    "twdump": "tailwind -o output.css"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^0.19.27",
    "@expo/metro-config": "^0.10.7",
    "@react-navigation/drawer": "^6.6.6",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^5.8.7",
    "axios": "^1.6.3",
    "clsx": "^2.1.0",
    "dotenv": "^16.3.1",
    "entities": "^3.0.1",
    "expo": "^49.0.21",
    "expo-auth-session": "~5.0.2",
    "expo-av": "~13.4.1",
    "expo-constants": "~14.4.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.14",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-web-browser": "~12.3.2",
    "lucide-react-native": "^0.300.0",
    "luxon": "^3.4.4",
    "mime": "^4.0.1",
    "nativewind": "^4.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-get-random-values": "^1.9.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "react-native-svg": "^13.9.0",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@brain2/eslint-config": "workspace:^0.2.0",
    "@brain2/prettier-config": "workspace:^0.1.0",
    "@brain2/tailwind-config": "workspace:^0.1.0",
    "@brain2/tsconfig": "workspace:^0.1.0",
    "@expo/config-plugins": "^7.2.5",
    "@types/babel__core": "^7.20.4",
    "@types/luxon": "^3.3.7",
    "@types/react": "^18.2.45",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tailwindcss": "3.4.0",
    "typescript": "^5.3.3",
    "uri-scheme": "^1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@brain2/eslint-config/base",
      "@brain2/eslint-config/react"
    ],
    "ignorePatterns": [
      "expo-plugins/**"
    ]
  },
  "prettier": "@brain2/prettier-config"
}
